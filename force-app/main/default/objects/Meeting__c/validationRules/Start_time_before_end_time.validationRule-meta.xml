<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_time_before_end_time</fullName>
    <active>true</active>
    <errorConditionFormula>Starts_at__c &gt;= Ends_at__c</errorConditionFormula>
    <errorMessage>Start time should be before end time and should be in one day.</errorMessage>
</ValidationRule>
